{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "outDir": "./disk",
        "types":["@cloudflare/workers-types"]
    },
    "include": ["**/*.ts", "scrape.TS", "src/server.js"],
    "exclude": ["node_modules"]
}